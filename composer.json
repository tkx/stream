{
    "name": "moteam/stream",
    "description": "PHP arrays and objects utility library",
    "type": "library",
    "keywords": [
        "array",
        "arrays",
        "object",
        "objects",
        "utility",
        "library"
    ],
    "autoload": {
        "psr-4": {
            "Moteam\\Stream\\": "src/"
        },
        "files": ["src/Library/functions.php"]
    },
    "authors": [
        {
            "name": "tkxyz",
            "email": "tk.moteam@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.6.x-dev",
        "phpstan/phpstan": "^1.10",
        "vimeo/psalm": "^5.15"
    },
    "scripts": {
        "test": "phpunit -v tests",
        "stan": "phpstan analyze src",
        "stan-l1": "phpstan analyze src --level 1"
    }
}
